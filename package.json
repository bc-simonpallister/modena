{
  "name": "@uniformdev/modena",
  "version": "8.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "dev:all": "run-s build export dev:worker",
    "dev:next": "next dev -p 4120",
    "dev": "run-s generate:intents dev:next",
    "build": "run-s generate build:next",
    "build:next": "next build",
    "export": "next export",
    "generate": "run-p generate:intents",
    "generate:intents": "uniform optimize manifest download --output ./lib/intentManifest.json --apiKey $UNIFORM_API_KEY",
    "start": "next start -p 4120",
    "ci:build": "run-s build export",
    "build:worker": "npx wrangler build",
    "dev:worker": "wrangler dev",
    "publish:worker": "npx wrangler publish",
    "yolo": "run-s build export build:worker publish:worker",
    "install:wrangler": "node ./scripts/install-wrangler.js",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "postinstall": "run-p install:wrangler",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
    "@analytics/segment": "^1.1.0",
    "@cloudflare/kv-asset-handler": "^0.1.1",
    "@uniformdev/optimize-edge-cloudflare": "^10.0.0",
    "@uniformdev/optimize-tracker": "^10.0.0",
    "@uniformdev/optimize-tracker-analytics": "^10.0.0",
    "@uniformdev/optimize-tracker-browser": "^10.0.0",
    "@uniformdev/optimize-tracker-common": "^10.0.0",
    "@uniformdev/optimize-tracker-react": "^10.0.0",
    "@uniformdev/upm": "^10.0.0",
    "@uniformdev/upm-bigcommerce": "^10.0.0",
    "@uniformdev/upm-contentful": "^10.0.0",
    "@uniformdev/upm-react": "^10.0.0",
    "analytics": "^0.7.5",
    "contentful": "^8.4.2",
    "next": "^11.0.1",
    "nookies": "^2.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@cloudflare/workers-types": "^2.2.2",
    "@contentful/rich-text-html-renderer": "^15.1.0",
    "@next/bundle-analyzer": "^11.0.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@uniformdev/cli": "8.0.2-alpha.30",
    "autoprefixer": "^10.2.5",
    "contentful-cli": "^1.8.23",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "modern-normalize": "^1.1.0",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
